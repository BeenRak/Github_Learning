<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="elearning.app.java"
	android:versionCode="1"
	android:versionName="1.0" >

	<uses-sdk android:minSdkVersion="7"  android:targetSdkVersion="13"  />
	<uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
	

	<application
		android:icon="@drawable/java"
		android:label="@string/app_name" >
		 <activity
            android:name="com.google.ads.AdActivity"
             android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|uiMode|screenSize|smallestScreenSize"  />
		<activity
			android:label="@string/app_name"
			android:name="elearning.app.java.Splash"  android:configChanges="keyboardHidden|orientation">
			<intent-filter >
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>  
		<activity
			android:label="@string/app_name"
			android:name="elearning.app.java.E_Learning_AppActivity"  android:configChanges="keyboardHidden|orientation">
			<intent-filter >
				<action android:name="android.intent.action.MAIN" />

				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
		</activity>
			<activity
			android:label="@string/app_name"
			android:name="elearning.app.java.Homepage_Activity" android:configChanges="keyboardHidden|orientation" >
			<intent-filter >
				<action android:name="android.intent.action.MAIN" />

				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
		</activity>
		
		
		<activity
			android:label="@string/app_name"
			android:name="elearning.app.java.AnsActivity" android:configChanges="keyboardHidden|orientation">
		</activity>
		
		<activity
			android:label="@string/app_name"
			android:name="elearning.app.java.Quiz_Activity" android:configChanges="keyboardHidden|orientation" >
			<intent-filter >
				<action android:name="android.intent.action.MAIN" />

				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
		</activity>
	</application>

</manifest>